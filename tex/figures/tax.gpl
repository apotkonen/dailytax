#set yrange [0:250]
set ytics
set xlabel "From 0 to income (i) cumulated consumption units group g(i) [1M]"
set xtics
set key center Right top nobox
a=58.82 # Y-scale "amplification" median
b=2.34012 # Shape
o=22.580040 # X-axis "location" i0 or origin
