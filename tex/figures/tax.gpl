U=5.428508 # Scale used units for CDF F(i), consumption Units ~ population/100000
set xrange [0:U]
#InvCDF(P,a,b)=a*((1-P)/P)**b # Inverse Cumulative Distribution Function Fisk
#InvF(g,a,b,U,o)=a*((1-g/U)/(g/U))**b+o # Inv Function
